{
	"schemaVersion": 1,
	"id": "chatai",
	"version": "3.0-debug",
	"name": "chatAI",
	"description": "这是一个将minecraft接入deepseek的模组，可以在游戏内直接与deepseek聊天，3.0版本还添加了AI玩家，更多有趣玩法在游戏体验。模组作者:xiaofan",
	"authors": [
		"xiaofan,Deepseek"
	],
	"contact": {
		"homepage": "https://github.com/xiaofanforfabric/ChatAIforfabric/",
		"sources": "https://github.com/xiaofanforfabric/ChatAIforfabric/"
	},
	"license": "CC0-1.0",
	"icon": "assets/chatai/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.xiaofan.chatai.ChatAI"
		],
		"client": [
			"com.xiaofan.chatai.ChatAIClient"
		],
		"fabric-datagen": [
			"com.xiaofan.chatai.ChatAIDataGenerator"
		]
	},
	"mixins": [
		"chatai.mixins.json",
		{
			"config": "chatai.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}